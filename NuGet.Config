<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageRestore>
    <add key="enabled" value="True" />
  </packageRestore>
  <!--
    this is used for boot-strapping build tools.  if you need to add a new
    nuget package source please add it to the item group in dir.props instead.
  -->
  <config>
    <add key="repositoryPath" value="..\packages" />
  </config>
  <packageSources>
    <add key="myget.org dotnet-core" value="https://dotnet.myget.org/F/dotnet-core/" />
    <add key="myget.org dotnet-buildtools" value="https://dotnet.myget.org/F/dotnet-buildtools/" />
    <add key="myget.org dotnet-corefxlab" value="https://dotnet..myget.org/F/dotnet-corefxlab/" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
</configuration>
